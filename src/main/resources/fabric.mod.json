	"authors": [
		"ShadowDragon"
	],
	"contact": {
		"homepage": "https://github.com/ShadowDragonMinecraft/colored-hex-blocks/wiki",
		"sources": "https://github.com/ShadowDragonMinecraft/colored-hex-blocks"
